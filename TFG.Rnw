repre <- function(var, perc){
  vector <- numeric()
  for(i in 1:length(var)){
    subs <- subset(bbdd,select=var[i])
    taula <- table(subs)/sum(!is.na(subs)) < perc
    if(any(taula)==TRUE){
      vector <- c(vector, i)
    }
  }
  return(vector)
}
